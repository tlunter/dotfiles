[user]
    name = Todd Lunter
    email = tlunter@gmail.com
[push]
    default = simple
[github]
    user = tlunter
[http]
    sslVerify = false
[core]
    editor = vim
    excludesfile = ~/.gitignore
[alias]
    l = log --graph
    c = commit --verbose
    d = diff
    s = status
    temporary-amnesia = update-index --skip-worktree
    remember = update-index --no-skip-worktree
[merge]
    ff = false
[rebase]
    pull = true
[color]
    ui = true
